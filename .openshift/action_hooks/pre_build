echo "installing node-gyp and other dependencies"
(cd "${OPENSHIFT_REPO_DIR}"; export TMPDIR="/tmp"; npm install -g node-gyp@3.2.0; export PYTHONPATH="${OPENSHIFT_REPO_DIR}node_modules/node-gyp/gyp"; npm install node-libs-browser@0.5.3; npm install webpack@1.9.x; npm install extract-text-webpack-plugin@1.0.1;)